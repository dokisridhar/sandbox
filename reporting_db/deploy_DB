scp  -o "StrictHostKeyChecking=no" -i ~/.oci/cen2020_pvt_key ./INSTALL_DB opc@$1:/tmp/.
scp  -o "StrictHostKeyChecking=no" -i ~/.oci/cen2020_pvt_key ./CONFIG_DB opc@$1:/tmp/.
ssh  -o "StrictHostKeyChecking=no" -i ~/.oci/cen2020_pvt_key  opc@$1 chmod 777 /tmp/CONFIG_DB
ssh  -o "StrictHostKeyChecking=no" -i ~/.oci/cen2020_pvt_key  opc@$1 chmod 777 /tmp/INSTALL_DB
ssh  -o "StrictHostKeyChecking=no" -i ~/.oci/cen2020_pvt_key  opc@$1 /tmp/CONFIG_DB
ssh  -o "StrictHostKeyChecking=no" -i ~/.oci/cen2020_pvt_key  opc@$1 rm /tmp/CONFIG_DB /tmp/INSTALL_DB
